{
    "manifest_version": 3,
    "name": "WebExtension Event Recorder",
    "short_name": "Recorder",
    "version": "1.0.0",
    "key": "",
    "permissions": [
      "webNavigation",
      "tabs",
      "windows",
      "webRequest"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*",
      "ws://*/*",
      "wss://*/*"
    ],
    "background": {
      "service_worker": "index.js",
      "scripts": ["index.js"]
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "test@test.com"
        }
    }
  }